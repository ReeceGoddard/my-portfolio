{
    "private": true,
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "dev:my-website": "turbo run dev --filter my-website",
        "dev:currency-dash": "turbo run dev --filter currency-dash",
        "dev:sensei-express": "turbo run dev --filter sensei-express*",
        "dev:verse": "turbo run dev --filter verse",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "test": "turbo run test",
        "test:watch": "turbo run test:watch"
    },
    "devDependencies": {
        "@turbo/gen": "^1.9.7",
        "eslint": "^7.32.0",
        "eslint-config-custom": "*",
        "prettier": "^2.5.1",
        "turbo": "^1.10.1"
    },
    "name": "my-portfolio",
    "packageManager": "npm@9.7.1",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "dependencies": {}
}
